{
  "name": "fast-food-clientside",
  "version": "1.0.0",
  "description": "This project uses React to consume an fast-food API",
  "main": "app.js",
  "scripts": {
    "test": "jest --detectOpenHandles --coverage",
    "test:watch": "jest --watch --coverage",
    "coverage": "jest --colors --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "./node_modules/eslint/bin/eslint.js  client/*.jsx",
    "start": "webpack-dev-server --mode development --hot --open",
    "build": "webpack --mode production",
    "babel": "babel app.js -d build",
    "start:heroku": "node build/app.js",
    "heroku-postbuild": "npm run babel && npm run build"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./client/tests/setupTests.js"
    ],
    "moduleNameMapper": {
      ".+\\.(png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewinsoul/Fast-Food-ClientSide.git"
  },
  "keywords": [
    "React",
    "Client-Side"
  ],
  "author": "Andrew Okoye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewinsoul/Fast-Food-ClientSide/issues"
  },
  "homepage": "https://github.com/andrewinsoul/Fast-Food-ClientSide#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "css-loader": "^1.0.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "html-webpack-plugin": "^5.5.0",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-typing-animation": "^1.6.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "fetch-mock": "^7.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "jest-css-modules": "^1.1.0",
    "jest-css-modules-transform": "^2.1.1",
    "jest-transform-stub": "^1.0.0",
    "moxios": "^0.4.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.2.3"
  }
}
